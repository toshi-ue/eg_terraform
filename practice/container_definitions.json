[
  {
    "name": "example",
    "image": "nginx:latest",
    "essential": true,
    "LogConfiguration":{
      "logDriver": "awslogs",
      "options":{
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "nginx",
        "awslogs-group": "/ecs/example"
      }
    },
    "portMappings":[
      {
        "protocol": "tcp",
        "containerPort": 80
      }
    ]
  }
]